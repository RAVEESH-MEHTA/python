fname = raw_input("Enter file name: ")
fh = open(fname)
count = 0
value = 0
for line in fh:
    if line.startswith("X-DSPAM-Confidence:"):
        pos = line.find('0')
        cnf = line[pos:(pos+6)]
        count = count + 1
        value = value + float(cnf)

print "Average spam confidence: " + str(value/count)
